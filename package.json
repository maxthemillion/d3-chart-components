{
  "name": "@maxthemillion/vued3-linechart",
  "version": "0.1.6",
  "description": "A vue component that provides a d3.js linechart.",
  "main": "./dist/vue-d3Linechart.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-package": "rimraf ./dist && webpack --config ./webpack.config.js"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "d3": "^5.15.0",
    "d3-array": "^2.4.0",
    "d3-dsv": "^1.2.0",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.2.3",
    "@vue/cli-plugin-eslint": "^4.2.3",
    "@vue/cli-service": "^4.2.3",
    "babel-eslint": "^10.1.0",
    "css-loader": "^3.4.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^5.1.0",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11",
    "webpack-cli": "^3.3.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/typescript@3.1.40"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm",
    "resolveModules": {
      "modulesDirectories": ["src"],
      "aliases": {
        "@": "src"
      }
    }
  }
}
